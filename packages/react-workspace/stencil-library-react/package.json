{
  "name": "@mawhea/stencil-library-react",
  "version": "0.0.1",
  "description": "React wrapper for stencil library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "node ./__tests__/react-library.test.js",
    "build": "npm run tsc",
