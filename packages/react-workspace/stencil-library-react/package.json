  ],
  "keywords": [
    "stencil",
    "react"
  ],
  "author": "mjwheatley",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mawhea/stencil-library": "0.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  }
}
